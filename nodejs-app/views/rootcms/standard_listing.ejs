										</div>
                                        <a href="javascript:void(0)" class="btn btn-danger btn-color padingmix  btn-icon-left btn-sm editorLink " style="display:none;"> <i class="fa fa-plus"></i>Enter a new</a>    
                  						<button class="btn btn-primary btn-circle" type="button" onclick="refresh_data()">  <i class="fa fa-refresh"></i></button>
                  					</div>
         						</div>
      						</div>
     					</div>
						
						
						
						<div class="visible-xs ">
						
							<div class="row">
								<div class="col-lg-6 col-sm-12 col-xs-12">
									<div>
										<form class="form-inline" method="get" action=""> 
											<input class="form-control xs-margin-btm10 searchFieldClass" required id="smallSearchField" name="keyword" placeholder="Search..." type="text" value="<%= keywordStr %>">
                     						<a href="javascript:void(0)" class="btn btn-danger btn-color padingmix btn-icon-left btn-sm editorLink" STYLE="width:100%; margin:10px 0px;display:none;"> <i class="fa fa-plus"></i>Enter a new</a>  
                  						</form>
									</div>
								</div>
							</div>
						</div>
	
	</div>
	
	</div>
   
      <div class="row">
	  
       
		
		
		
		<div class="content table-responsive table-full-width">
		 <div class="table-responsive" style="border:none;" >
                                <table class="table table-striped  table-bordered table-hover custom-tbl-st bt" id="table-breakpoint"  style="background-color: rgb(255, 255, 255); border-radius: 4px;" >
                                    <thead class="headFootClass">
                                       
                                    </thead>
                                     <tfoot class="headFootClass">
                                       
                                    </tfoot>
                                    <tbody id="documents_data">
                                    	
                                    </tbody>
                                </table>
                            </div>
		
		
						
					</div>
		
		
		
        <!-- right col -->
      </div>
      <!-- /.row (main row) -->

    </section>

  </div>
 
 <!--//Main Content--> 
	<!-- Footer Starts Here-->
	<% include include/footer %>
<!-- Footer Ends Here-->
<script src="js/basictable.js"></script>
<script type="text/javascript">
var templateStr="<%= currentTemplateStr %>", searchStr="<%= keywordStr %>";
